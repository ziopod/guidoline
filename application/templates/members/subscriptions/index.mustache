<nav class="nav_edit">
	<ul>
		<li>
			<a class="btn btn-info tip" href="{{base_url}}members/subscriptions_add/{{member.id}}"><i class="icon-plus"></i> Ajouter une inscription</a>
		</li>
	</ul>
</nav>
<h1>{{member.firstname}} {{member.name}}</h1>
<p>
	Adhérent depuis le : {{member.first_subscription.created}}
</p>
<table class="table">
	<thead>
		<tr>
			<th>Titre</th>
			<th>Tarif</th>
			<th>Soucrit le</th>
			<th>Valide</th>
			<th>Action</th>
		</tr>
	</thead>
	<tbody>
{{#subscriptions_member}}

	<tr>
		<td>{{subscription.title}}</td>
		<td>{{subscription.cost}}€</td>
		<td>{{created}}</td>
		<td>
			{{#valid_subscription}}
				Oui
			{{/valid_subscription}}
			{{^valid_subscription}}
				Non
			{{/valid_subscription}}
		</td>
		<td>
			<a class="icon-trash tip" href="{{base_url}}members/subscription_delete/{{id}}" title="Supprimer"></a>
		</td>
	</tr>
{{/subscriptions_member}}
{{^subscriptions_member}}
<tr>
	<td colspan="6"><strong>Aucune inscription actuellement</strong></td>
</tr>
{{/subscriptions_member}}
	</tbody>
</table>
