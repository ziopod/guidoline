{{#member}}
<h1>{{firstname}} {{name}}</h1>
<ul>
	<li>Créer le : {{created}}</li>
	<li>Première inscription le : {{member.created}}</li>
	<li>Statut : {{member.status.name}}</li>
	<li>Email : {{user.email}}</li>
	<li>
		Adresse :
		<br />
		{{member.street}}
		<br />
		{{member.zipcode}}
		<br />
		{{member.city}}
	</li>
</ul>
{{/member}}
<a href="{{base_url}}users/profil/{{member.user.id}}">Affiche la fiche utilisateur</a>