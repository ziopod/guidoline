<tr {{^member.is_active}}class="faded"{{/member.is_active}}>
  <td class="is-hidden-mobile">#{{member.idm}}</td>
  <td>
    <a
      class="member_set_volunteer is-hidden-mobile icon is-medium {{#member.is_volunteer}}is-active{{/member.is_volunteer}}{{^member.is_volunteer}}is-inactive{{/member.is_volunteer}} ajax-action"
      data-action_do="member_set_volunteer"
      data-action_id="{{member.id}}"
      title="Marquer comme {{#member.is_volunteer}}non-{{/member.is_volunteer}}bénévole">
      <img class="icon-injector" src="/assets/svg/crown.svg" alt="Bénévole">
    </a>
    <a href="{{member.url}}" title="Afficher la fiche de {{member.fullname}}">
      {{member.fullname}}
    </a>
  </td>
  <td class="is-hidden-mobile">
    {{#member.email}}
    <a href="mailto:{{member.email}}?body=Bonjour {{member.firstname}},&subject=Message de Guidoline" title="Écrire à {{member.fullname}}">
      <span class="icon is-medium">
        <img class="icon-injector" src="/assets/svg/mail.svg" alt="{{member.email}}">
      </span>
    </a>
    {{/member.email}}
  </td>
  <td class="nowrap">
    {{#member.email}}
    <a href="mailto:{{member.email}}?body=Bonjour {{member.firstname}},&subject=Message de Guidoline" title="Écrire à {{member.fullname}}">
      <span class="is-hidden-tablet icon is-medium">
        <img class="icon-injector" src="/assets/svg/mail.svg" alt="Email">
      </span>
    </a>
    {{/member.email}}
    {{#member.phone}}
    <a href="tel:+{{member.phone}}">
      <span class="is-hidden-tablet icon is-medium">
        <img class="icon-injector" src="/assets/svg/call.svg" alt="Téléphone">
      </span>
      <span class="nowrap is-hidden-mobile">{{member.phone}}</span>
    </a>
    <a class="is-hidden-tablet" href="sms:+{{member.phone}}?body=Bonjour {{member.firstname}},%0a">
      <span class="is-hidden-tablet icon is-medium">
        <img class="icon-injector" src="/assets/svg/text.svg" alt="SMS">
      </span>
    </a>
    {{/member.phone}}
  </td>
  <td class="is-hidden-mobile">
  {{#member.dues.records_count}}
  {{#member.dues.records}}
  <a href="{{due.url}}" title="Afficher la cotisation n° {{due.id}} de {{member.fullname}}">
    {{due.title}}
  </a>
  {{/member.dues.records}}
  {{/member.dues.records_count}}
  {{^member.dues.records_count}}
  <a href="{{member.url_edit}}#dues">Ajouter une adhésion</a>
  {{/member.dues.records_count}}
  </td>
</tr>
